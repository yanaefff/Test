<?xml version="1.0" encoding="Windows-1251"?>
<xs:schema version="1.0" xmlns="http://schema.pfr.ru" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://schema.pfr.ru" elementFormDefault="qualified">

	<xs:annotation>
		<xs:documentation xml:lang="ru">XML-подсхема для проверки документов - Типы данных</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="ТипИмяФайла">
		<xs:annotation>
			<xs:documentation>Тип для представления имени XML-файла в информационных обменах ПФР</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="PFR-700-Y-\d{4}-ORG-\d{3}-\d{3}-\d{6}-DCK-\d{5}-DPT-\d{6}-DCK-\d{5}.XML"/>
			<xs:pattern value="OUT-700-Y-\d{4}-ORG-\d{3}-\d{3}-\d{6}-DCK-\d{5}-DPT-\d{6}-DCK-\d{5}.XML"/>
			<xs:pattern value="OUT-700-Y-\d{4}-ORG-\d{3}-\d{3}-\d{6}-DCK-\d{5}-OUTDPT-\d{3}-\d{3}-OUTNMB-\d{4}-\d{9}.XML"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ТипДата">
		<xs:annotation>
			<xs:documentation>Дата в формате дд.мм.гггг</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(((31\.(0[13578]|1[02]))|((29|30)(\.)(0[1,3-9]|1[0-2])))\.((1[6-9]|[2-9]\d)\d{2}))|(29\.02\.((((1[6-9]|[2-9]\d)(0[48]|[2468][048]|[13579][26]))|((16|[2468][048]|[3579][26])00))))|((0[1-9]|1\d|2[0-8])\.((0[1-9])|(1[0-2]))\.((1[6-9]|[2-9]\d)\d{2}))"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипДатаН">
		<xs:annotation>
			<xs:documentation>Дата в формате дд.мм.гггг (или пустое значение)</xs:documentation>
		</xs:annotation>

		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="\d{2}\.\d{2}\.\d{4}"/>
			<xs:pattern value=""/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипВсеГоды">
		<xs:annotation>
			<xs:documentation>Тип для представления количества лет выработки (стажа)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99"/>
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипВсеМесяцы">
		<xs:annotation>
			<xs:documentation>Тип для представления количества месяцев выработки (стажа)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="12"/>
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипВсеДни">
		<xs:annotation>
			<xs:documentation>Тип для представления количества дней выработки (стажа)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="29"/>
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипНомерКорректировки">
		<xs:annotation>
			<xs:documentation>Тип для представления номера корректировки</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="3"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>

