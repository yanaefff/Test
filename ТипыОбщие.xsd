		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество">
		<xs:annotation>
			<xs:documentation>Тип для представления количественных показателей (до 3 знаков)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество4">
		<xs:annotation>
			<xs:documentation>Тип для представления количественных показателей (до 4 знаков)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипМесяц">
		<xs:annotation>
			<xs:documentation>Тип для представления месяца в году: от 1 (январь) до 12 (декабрь)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипДес1">
		<xs:annotation>
			<xs:documentation>Тип для представления числа от 0 до 1 с сотыми долями</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2"/>
