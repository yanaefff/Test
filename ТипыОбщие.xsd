		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="7"/>
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипЧисло8">
		<xs:annotation>
			<xs:documentation>Число 8 разрядов</xs:documentation>
		</xs:annotation>

		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="8"/>
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99999999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипЧисло9">
		<xs:annotation>
			<xs:documentation>Число 9 разрядов</xs:documentation>
		</xs:annotation>

		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="9"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипЧисло10">
		<xs:annotation>
			<xs:documentation>Число 10 разрядов</xs:documentation>
		</xs:annotation>

		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="10"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипНомерПачки">
		<xs:annotation>
			<xs:documentation>Порядковый номер пачки</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99999"/>
			<xs:totalDigits value="5"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипИНН">
		<xs:annotation>
			<xs:documentation>Тип для представления идентификационного номера налогоплательщика (ИНН)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}"/>
